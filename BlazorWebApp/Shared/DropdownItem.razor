<li><button type="button" class="dropdown-item @(Parent.ButtonText == Text ? "active" : "")" @onkeypress:preventDefault @onclick="e => Parent.HandleSelected(Text)">@Text</button></li>

@code {
	[CascadingParameter(Name = "Dropdown")]
	public Dropdown Parent { get; set; }

	[Parameter]
	public string Text { get; set; }
}
