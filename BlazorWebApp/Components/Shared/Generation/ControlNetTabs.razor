@inject ManagerService M

<MudText Typo="Typo.button" Style="opacity:0.2;">Control Net</MudText>
<MudTabs PanelClass="px-4 py-6" Elevation="4" Rounded=true ApplyEffectsToContainer=true>
    @{
        var index = 1;
        @foreach (var tab in Units)
        {
            <MudTabPanel Text=@($"Unit {index}")>
                <ControlNetForm Parameters="@tab" />
            </MudTabPanel>
            index++;
        }
    }
</MudTabs>

@code {
    [Parameter] public List<ScriptParametersControlNet> Units { get; set; }
}
